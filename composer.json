{
    "name": "fonsecas72/behat-selenium-starter",
    "type": "behat-extension",
    "description": "Automatically downloads and starts selenium server so you don't need to worry about it in your behat-mink tests.",
    "keywords": ["behat", "start", "download"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hugo Fonseca",
            "email": "hdtfonseca@gmail.com"
        }
    ],
    "require": {
        "fonsecas72/selenium-handler": "~0.0.3"
    },    
    "autoload": {
        "psr-4": {
            "Fonsecas72\\SeleniumStarterExtension\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
